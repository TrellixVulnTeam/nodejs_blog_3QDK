<h1>Tất cả sản phẩm</h1>
<div class="row mx-0">
    {{#each data}}
    <div class="col-12 col-sm-4 col-md-3 col-lg-3">
        <div class="card">
            {{#each this.product_image.image}}
                <li>{{this}}</li>
            {{/each}}
            {{this.product_image}}
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">{{this.product_name}}</h5>
                <p class="card-text">Giá: {{this.product_price}}</p>
                <a href="#" class="btn btn-primary">Go somewhere</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>
